package BIOSKOP;

public class cKasir {
    private String id;
    private String nama;
    private String loket;
    
    //method
    //constructor
    cKasir (String i, String n){
    id = i; nama=n;
        System.out.println("Kasir "+nama+" dibuat...");    
    }
    
    //setter 
    void setId(String i){ id=i; }
    void setNama(String nm){ nama=nm; }
    void setLoket(String l){ loket=l; }

    
    //getter
    String getId(){ return id; }
    String getLoket(){ return loket; }
    String getNama(){ return nama; }
    
}
